<?xml version="1.0" ?>
<decorators defaultdir="/WEB-INF/decorators">
	<excludes>
		<pattern>/index.jsp</pattern>
		<pattern>/nosession.jsp</pattern>
		<pattern>/app/inicio</pattern>
		<pattern>/app/inicio/</pattern>
		<pattern>/app/listados/parametros/*</pattern>
		<pattern>/app/informes/observaciones/*</pattern>
		<pattern>/app/documentos/busquedaEntidad/*</pattern>
		<pattern>/app/documentos/busquedaContrato/*</pattern>
		<pattern>/app/archivo/*</pattern>
	</excludes>
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>